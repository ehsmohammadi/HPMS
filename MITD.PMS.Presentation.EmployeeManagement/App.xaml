<Application xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:MITD.PMS.Presentation.Logic"
             x:Class="MITD.PMS.Presentation.EmployeeManagement.App"
             >
    <Application.Resources>
        <ResourceDictionary>
            <vm:EmployeeListVM x:Key="elViewModel"/>
            <vm:EmployeeVM x:Key="eViewModel"/>
            <vm:EmployeeJobPositionsVM x:Key="ejpViewModel"/>
            <vm:EmployeeJobCustomFieldsVM x:Key="ejCustomFieldsViewModel" />
            

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Assets/LocalResource.xaml"/>
                <ResourceDictionary Source="/MITD.PMS.Presentation.UI.Common;component/Assets/LocalResource.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
