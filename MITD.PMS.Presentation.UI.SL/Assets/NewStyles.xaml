                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="FocusStates">
                                <vsm:VisualState x:Name="Focused" />
                                <vsm:VisualState x:Name="Unfocused" />
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="ExpansionStates">
                                <vsm:VisualState x:Name="Expanded">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="theOutlookBarButtonContentPresenter" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Collapsed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="theOutlookBarButtonContentPresenter" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="SelectionStates">
                                <vsm:VisualState x:Name="Selected">
                                    <Storyboard>
                                        <ColorAnimation Duration="00:00:00.001" Storyboard.TargetName="ContentControlParent" Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" To="White"/>
                                        
                                        <ColorAnimationUsingKeyFrames  Duration="00:00:00.001" Storyboard.TargetName="OutlookBarButtonGrid"
                                                                      Storyboard.TargetProperty="(UIElement.Background).(GradientBrush.GradientStops)[0].(GradientStop.Color)">
                                            <SplineColorKeyFrame KeyTime="0" Value="#2ebef8" />
                                        </ColorAnimationUsingKeyFrames>
                                        <ColorAnimationUsingKeyFrames Duration="00:00:00.001" Storyboard.TargetName="OutlookBarButtonGrid"
                                                                      Storyboard.TargetProperty="(UIElement.Background).(GradientBrush.GradientStops)[1].(GradientStop.Color)">
                                            <SplineColorKeyFrame KeyTime="0" Value="#0f83c1" />
                                        </ColorAnimationUsingKeyFrames>
                                        <ColorAnimationUsingKeyFrames Duration="00:00:00.001" Storyboard.TargetName="OutlookBarButtonGrid"
                                                                      Storyboard.TargetProperty="(UIElement.Background).(GradientBrush.GradientStops)[2].(GradientStop.Color)">
                                            <SplineColorKeyFrame KeyTime="0" Value="#125386" />
                                        </ColorAnimationUsingKeyFrames>
                                        <ColorAnimationUsingKeyFrames Duration="00:00:00.001" Storyboard.TargetName="OutlookBarButtonGrid"
                                                                      Storyboard.TargetProperty="(UIElement.Background).(GradientBrush.GradientStops)[3].(GradientStop.Color)">
                                            <SplineColorKeyFrame KeyTime="0" Value="#1f97cb" />
                                        </ColorAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Unselected" />
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        <Border BorderBrush="{StaticResource OutlookBarButtonBorderBrush}" HorizontalAlignment="Stretch" BorderThickness="0,0,0,1">
                            <ContentControl x:Name="ContentControlParent" Foreground="#013a51" >
                           
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="0,0,0,0">
                                    <Image x:Name="theOutlookBarButtonImage" Source="{TemplateBinding HeaderImage}" Width="Auto" Height="Auto" Margin="2" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                <ContentPresenter x:Name="theOutlookBarButtonContentPresenter" 
                                                  Margin="0,0,0,0"
                                                  
                                                  Content="{TemplateBinding Header}" 
                                                  VerticalAlignment="Center" />
                            </StackPanel>
                           
                        </ContentControl>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <!-- OutlookBar -->
    <Style TargetType="outlook:OutlookBar">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Background" Value="{StaticResource OutlookBarBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{StaticResource OutlookBarBorderBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="ItemContainerStyle" Value="{StaticResource OutlookBarButton}" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel x:Name="OutlookBarStackPanel" Orientation="Vertical" Height="Auto" Margin="0,10,0,0" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="IsExpanded" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="outlook:OutlookBar">
                    <Border x:Name="OutlookBorder" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualStateGroup.Transitions>
                                    <vsm:VisualTransition GeneratedDuration="00:00:00.2" />
                                </vsm:VisualStateGroup.Transitions>
                                <vsm:VisualState x:Name="Normal" />
                                <vsm:VisualState x:Name="MouseOver" />
                                <vsm:VisualState x:Name="Disabled" />
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="ExpansionStates">
                                <vsm:VisualState x:Name="Expanded" />
                                <vsm:VisualState x:Name="Collapsed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="VerticalButton" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="theOutlookBarContentPresenter" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="InternalControlTemplateGrid" Storyboard.TargetProperty="Width" Duration="0">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="34"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="InternalControlTemplateGrid" Storyboard.TargetProperty="HorizontalAlignment" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Left" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HeaderTextBlock" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="OutlookBorder" Storyboard.TargetProperty="Width" Duration="0">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="34"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="OutlookBorder" Storyboard.TargetProperty="HorizontalAlignment" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Left" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        <Grid x:Name="ControlTemplateGrid">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="30" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0" Height="30" Background="{StaticResource OutlookHeaderBrush}" HorizontalAlignment="Stretch">
                                <TextBlock x:Name="HeaderTextBlock" FontStretch="UltraExpanded" HorizontalAlignment="Left" VerticalAlignment="Center" TextWrapping="Wrap"
                                            Text="{TemplateBinding Header}"
                                            FontSize="13" FontWeight="SemiBold" Margin="5" Foreground="{StaticResource OutlookCaptionBrush}"
                                           Visibility="Visible"/>
                                <ToggleButton x:Name="theOutlookToggleButton" HorizontalAlignment="Right" Width="28" Height="28" Style="{StaticResource OutlookDoubleArrowToggleButtonStyle}" />
                            </Grid>
                            <Grid x:Name="InternalControlTemplateGrid" Visibility="Visible" Grid.Row="1" HorizontalAlignment="Stretch">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="Auto" MinHeight="10" MaxHeight="{Binding ElementName=theOutlookBarButtonsPresenter, Path=ActualHeight}" />
                                </Grid.RowDefinitions>
                                <ContentPresenter x:Name="theOutlookBarContentPresenter" Grid.Row="0" 
                                                    Content="{TemplateBinding SelectedContent}"
                                                    FlowDirection="{TemplateBinding FlowDirection}" 
                                                    Opacity="{TemplateBinding Opacity}" 
                                                    Visibility="Visible"/>
                                <Button x:Name="VerticalButton" Visibility="Collapsed" Width="34" Height="Auto" Style="{StaticResource VerticalButtonStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Left" Grid.Row="0" />
                                <outlook:OutlookBarGridSplitter x:Name="theOutlookBarGridSplitter" DragIncrement="30" Background="{StaticResource OutlookGridSplitterBrush}" Grid.Row="1" Height="10" HorizontalAlignment="Stretch" Margin="0" VerticalAlignment="Top"/>
                                <ItemsPresenter x:Name="theOutlookBarButtonsPresenter" Visibility="Visible" VerticalAlignment="Top" Grid.Row="1" />
                            </Grid>
                            <Grid Grid.Row="2" Height="30" Background="{StaticResource OutlookFooterBrush}" HorizontalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="34" />
                                </Grid.ColumnDefinitions>
                                <StackPanel x:Name="theOutlookBarFooterTrayStackPanel" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right" />
                                <Button x:Name="theConfigureButton" Grid.Column="1" Style="{StaticResource ConfigureButton}">
                                    <Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                        <Path Data="M116,112 L69,45 L164,45 z" Fill="{StaticResource OutlookBarBorderBrush}" Margin="5, 10" Stretch="Fill" Stroke="{StaticResource OutlookBarBorderBrush}" UseLayoutRounding="False"/>
                                    </Grid>
                                </Button>
                            </Grid>
                        </Grid>
                    </Border>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <!-- OutlookBarGridSplitter -->
    <Style TargetType="outlook:OutlookBarGridSplitter">
        <Setter Property="Background" Value="#FFFFFFFF"/>
        <Setter Property="IsTabStop" Value="true"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="PreviewStyle">
            <Setter.Value>
                <Style TargetType="Control">
                    <Setter Property="Background" Value="#FF868686"/>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="Control">
                                <Grid x:Name="Root" Opacity=".5">

                                    <!-- Background -->
                                    <Rectangle Fill="{TemplateBinding Background}" />

                                    <!-- Horizontal Template -->
                                    <Grid x:Name="HorizontalTemplate" Height="6">
                                        <!-- Just show the faint gray grid splitter rectangle with no other details -->
                                    </Grid>

                                    <!-- Vertical Template -->
                                    <Grid x:Name="VerticalTemplate" Visibility="Collapsed" Width="6">
                                        <!-- Just show the faint gray grid splitter rectangle with no other details -->
                                    </Grid>

                                </Grid>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="outlook:OutlookBarGridSplitter">
                    <Grid x:Name="Root" IsHitTestVisible="{TemplateBinding IsEnabled}">

                        <!-- VSM -->
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualState x:Name="Normal" />
                                <vsm:VisualState x:Name="MouseOver" />
                                <vsm:VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="0.5" Duration="0" />
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="FocusStates">
                                <vsm:VisualStateGroup.Transitions>
                                    <vsm:VisualTransition GeneratedDuration="0" />
                                </vsm:VisualStateGroup.Transitions>
                                <vsm:VisualState x:Name="Unfocused" />
                                <vsm:VisualState x:Name="Focused">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="FocusVisual" Storyboard.TargetProperty="Opacity" To="1" Duration="0" />
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>

                        <!-- Background -->
                        <Rectangle Fill="{TemplateBinding Background}" StrokeThickness="0" />

                        <!-- Horizontal Template -->
                        <Grid x:Name="HorizontalTemplate" Height="10">
                            <StackPanel Height="6" VerticalAlignment="Center">
                                <Rectangle Height="1" Margin="1" Width="10" StrokeThickness="0" Fill="#FF868686" />
                                <Rectangle Height="1" Margin="1" Width="10" StrokeThickness="0" Fill="#FF868686" />
                            </StackPanel>
                        </Grid>

                        <!-- Vertical Template -->
                        <Grid x:Name="VerticalTemplate" Visibility="Collapsed" Width="10">
                            <StackPanel Width="6" VerticalAlignment="Center" Orientation="Horizontal">
                                <Rectangle Width="1" Margin="1" Height="10" StrokeThickness="0" Fill="#FF868686" />
                                <Rectangle Width="1" Margin="1" Height="10" StrokeThickness="0" Fill="#FF868686" />
                            </StackPanel>
                        </Grid>

                        <!-- Focus Visual -->
                        <Rectangle x:Name="FocusVisual" Stroke="#FF6DBDD1" StrokeThickness="1" Opacity="0" IsHitTestVisible="false" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>




</ResourceDictionary>

